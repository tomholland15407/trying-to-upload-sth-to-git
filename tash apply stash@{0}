[32m+[m[32m    if n == 0:[m
[32m+[m[32m        return 1[m
[32m+[m[32m    else:[m
[32m+[m[32m        return n * factorial(n - 1)[m
[32m+[m[32mdef c(n, r):[m
[32m+[m[32m    return factorial(n) / (factorial(r) * factorial(n - r))[m
[32m+[m[32ma=10[m
[32m+[m
[32m+[m[32mfor i in range(0, a):[m
[32m+[m[32m    for t in range(0, i+1):[m
[32m+[m[32m        print(c(a, t), end=' ')[m
[32m+[m[32m    print("\n")[m
\ No newline at end of file[m
