[32m+[m[32mdef fact(n):[m
[32m+[m[32m    if n == 0:[m
[32m+[m[32m        return 1[m
[32m+[m[32m    else:[m
[32m+[m[32m        return n * fact(n-1)[m
[32m+[m[32mdef combination(a,b):[m
[32m+[m[32m    return fact(a)//(fact(a-b)*fact(b))[m
[32m+[m[32mn = int(input('enter a number: '))[m
[32m+[m[32mfor i in range(n):[m
[32m+[m[32m    print('  '*(n-i), end='')[m
[32m+[m[32m    for j in range(i+1):[m
[32m+[m[32m        t = combination(i,j)[m
[32m+[m[32m        print(f'{t:3}', end=' ')[m
[32m+[m[32m    print()[m
