[32m+[m[32mdef compress(s):[m
[32m+[m[32m    result = ''[m
[32m+[m[32m    i = 0[m
[32m+[m[32m    while i < len(s):[m
[32m+[m[32m        count = 1[m
[32m+[m[32m        while i < (len(s)-1) and s[i] == s[i+1]:[m
[32m+[m[32m            count += 1[m
[32m+[m[32m            i += 1[m
[32m+[m[32m        result += s[i] + str(count)[m
[32m+[m[32m        i += 1[m
[32m+[m[32m    return result[m
[32m+[m[32ms = input()[m
[32m+[m[32md = compress(s)[m
