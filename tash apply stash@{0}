[1m+++ b/integer partition.py[m	
[36m@@ -0,0 +1,22 @@[m
[32m+[m[32mdef generate_partitions(n, max_num=None):[m
[32m+[m[32m    if max_num is None:[m
[32m+[m[32m        max_num = n[m
[32m+[m[32m    if n == 0:[m
[32m+[m[32m        return [[]][m
[32m+[m[32m    if n < 0 or max_num == 0:[m
[32m+[m[32m        return [][m
[32m+[m
[32m+[m[32m    # Partitions that include max_num[m
[32m+[m[32m    with_max = generate_partitions(n - max_num, max_num)[m
[32m+[m[32m    with_max = [[max_num] + part for part in with_max][m
[32m+[m
[32m+[m[32m    # Partitions that exclude max_num[m
[32m+[m[32m    without_max = generate_partitions(n, max_num - 1)[m
[32m+[m
[32m+[m[32m    return with_max + without_max[m
[32m+[m
[32m+[m
[32m+[m[32m# Example usage[m
[32m+[m[32mpartitions = generate_partitions(2)[m
[32m+[m[32mfor p in partitions:[m
[32m+[m[32m    print(p)[m
