[32m+[m[32mdef prime(n):[m
[32m+[m[32m    if n == 1:[m
[32m+[m[32m        return False[m
[32m+[m[32m    else:[m
[32m+[m[32m        r = True[m
[32m+[m[32m        while r:[m
[32m+[m[32m            for i in range(2, n):[m
[32m+[m[32m                if n % i == 0:[m
[32m+[m[32m                    return False[m
[32m+[m
[32m+[m[32m            break[m
[32m+[m[32m        return True[m
[32m+[m
[32m+[m[32mdef factors(n):[m
[32m+[m[32m    factors = {}[m
[32m+[m[32m    power = 0[m
[32m+[m[32m    for i in range(2, n):[m
[32m+[m[32m        if prime(i) == True and n % i == 0:[m
[32m+[m[32m            for k in range(1,n):[m
[32m+[m[32m                if n % (i**k) == 0:[m
[32m+[m[32m                    power += 1[m
[32m+[m[32m            factors[i] = power[m
[32m+[m[32m            power = 0[m
[32m+[m[32m    return factors[m
[32m+[m[32mdict_1 = factors(50)[m
[32m+[m[32mdef prime_format(fact_dict):[m
[32m+[m[32m    return ' * '.join(f'{prime}^{power}' for prime, power in fact_dict.items())[m
[32m+[m[32mprint(prime_format(dict_1))[m
[32m+[m[32mdict_2 = factors(75)[m
[32m+[m[32mprint(prime_format(dict_2))[m
[32m+[m
[32m+[m[32mgcd = {}[m
[32m+[m[32mfor i in dict_1:[m
[32m+[m[32m    if i in dict_2:[m
[32m+[m[32m        gcd[i] = min(dict_1[i], dict_2[i])[m
[32m+[m[32ma = 1[m
[32m+[m[32mfor p in gcd:[m
[32m+[m[32m    a *= p**gcd[p][m
[32m+[m[32mprint(a)[m
[32m+[m
[32m+[m[32mfor i in dict_2:[m
[32m+[m[32m    if i in dict_1:[m
[32m+[m[32m        dict_1[i] = max(dict_1[i], dict_2[i])[m
[32m+[m[32m    else:[m
[32m+[m[32m        dict_1[i] = dict_2[i][m
[32m+[m
[32m+[m[32mt = 1[m
[32m+[m[32mfor i in dict_1:[m
[32m+[m[32m    t *= i**dict_1[i][m
[32m+[m[32mprint(t)[m
[32m+[m
[32m+[m[32mgcd_dict = {}[m
[32m+[m[32mfor p in dict_1:[m
[32m+[m[32m    if p in dict_2:[m
[32m+[m[32m        gcd_dict[p] = min(dict_1[p], dict_2[p])[m
[32m+[m
