[32m+[m[32m        if event.type == pygame.QUIT:[m
[32m+[m[32m            running = False[m
[32m+[m
[32m+[m[32m        elif event.type == pygame.KEYDOWN and event.key == pygame.K_SPACE:[m
[32m+[m[32m            space_pressed_time = time.time()[m
[32m+[m
[32m+[m[32m        elif event.type == pygame.KEYUP and event.key == pygame.K_SPACE:[m
[32m+[m[32m            if space_pressed_time:[m
[32m+[m[32m                duration = time.time() - space_pressed_time[m
[32m+[m[32m                symbol = 'Â·' if duration < threshold else 'â€“'[m
[32m+[m[32m                current_letter += symbol[m
[32m+[m[32m                morse_sequence += symbol + '   '[m
[32m+[m[32m                space_pressed_time = None[m
[32m+[m[32m                last_input_time = time.time()[m
[32m+[m
[32m+[m
[32m+[m[32m    if current_letter and (time.time() - last_input_time > letter_pause):[m
[32m+[m[32m        translated_char = MORSE_DICT.get(current_letter, '?')[m
[32m+[m[32m        translated_text += translated_char[m
[32m+[m[32m        current_letter = ""[m
[32m+[m[32m        morse_sequence += '       '[m
[32m+[m
[32m+[m[32m    morse_text = font.render(morse_sequence, True, BLACK)[m
[32m+[m[32m    translated = font.render("Translation: " + translated_text, True, BLACK)[m
[32m+[m[32m    screen.blit(morse_text, (20, 80))[m
[32m+[m[32m    screen.blit(translated, (20, 150))[m
[32m+[m[32m    pygame.display.flip()[m
[32m+[m
[32m+[m[32mpygame.quit()[m
